{
    "plugins": ["prettier", "import"],
    "extends": [
        "plugin:prettier/recommended",
        "prettier",
        "@typescript-eslint: tsEslint.plugin"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "amd": true
    },
    "rules": {
        "no-debugger": "off",
        "no-console": 0,
        "class-methods-use-this": "off",
        "@typescript-eslint/no-explicit-any": "error",
        "@typescript-eslint/no-require-imports":"off"
       
    },
    "globals": {
        "module": true
    }
}

